{
  "private": true,
  "name": "lordaeron",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "test:pkg": "jest ./tests -u",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "eslint": "eslint docs/ packages/concis-react/src --cache --fix --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint {packages,docs}/**/*.less --fix",
    "prettier": "prettier --config .prettierrc.js --write \"{packages,docs}/**/*.{js,jsx,ts,tsx}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eternallycyf/my-demo-markdown",
    "branch": "main"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "core-js": "^3.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@types/d3": "^7.4.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/three": "0.143.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "dumi": "^2.0.7",
    "ejs": "^3.1.6",
    "gh-pages": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "^10.4.0",
    "yorkie": "^2.0.0"
  }
}